{
    "name": "bauerdot/laravel-dbml",
    "description": "Kacher is Laravel DB to DBML (dbdiagram.io/dbdocs.io)",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Arsanandha Aphisitworachorch",
            "email": "arsanandha.ap@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.37|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Bauerdot\\LaravelDbml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bauerdot\\LaravelDbml\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bauerdot\\LaravelDbml\\LaravelDbmlServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    }
}
